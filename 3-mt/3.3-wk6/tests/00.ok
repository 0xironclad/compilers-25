program list_test
  integer list a;
  integer list b;
  integer list c;
  integer x;
  integer y;
begin
  a := [1, 2, 3];
  b := [4, 5, 6];
  x := 10;
  y := 20;
  c := [x, y, x + y, 100];
  x := a[1];
  y := b[2];
  write(x);
  write(y);
  a[2] := 999;
  write(a[2]);
  a := b ++ [100, 10, 1];
  c := [1, 2] ++ [3, 4];
